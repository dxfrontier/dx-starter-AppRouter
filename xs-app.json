{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "routes": [
     {
      "source": "^srv/(.*)$",
      "target": "$1",
      "destination": "srv",
      "csrfProtection": true,
      "authenticationType": "ias"
    },
    {
      "source": "^(.*)$",
      "localDir": "web",
      "authenticationType": "ias"
    }
  ]
}